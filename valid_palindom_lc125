public boolean isPalindrome(String s) {
    if (s.isEmpty()) {
        return true;
    }
    s = s.replaceAll(" ", "");
    s = s.replaceAll("[^A-Za-z0-9]", "");
    s = s.toLowerCase();
    System.out.println(s);
    for (int i = 0; i < (s.length() / 2); i++) {
        int lenMath = s.length() - 1;
        if (s.charAt(i) != s.charAt(lenMath - i)) {
            return false; 
        }
    }
    return true;
}